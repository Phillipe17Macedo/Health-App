{
  "expo": {
    "name": "health-app",
    "slug": "health-app",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon-aserpa.png",
    "scheme": "health-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/aserpa-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-aserpa.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.phillipe17macedo.healthapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon-aserpa.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ffd01dd6-8e1a-45a7-9104-c92bcbc71b3e"
      }
    },
    "owner": "phillipe17macedo"
  }
}
