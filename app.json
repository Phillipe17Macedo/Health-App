{
  "expo": {
    "name": "Aserpa Sa√∫de",
    "slug": "health-app",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon-aserpa.png",
    "scheme": "aserpa-saude",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/aserpa-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phillipe17macedo.healthapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-aserpa.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.phillipe17macedo.healthapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon-aserpa.png"
    },
    "plugins": [
      "expo-router",
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ffd01dd6-8e1a-45a7-9104-c92bcbc71b3e"
      },
      "oneSignalAppId": "45d50449-03b9-463e-a6a7-e6c1258cfd7d"
    },
    "owner": "phillipe17macedo"
  }
}
