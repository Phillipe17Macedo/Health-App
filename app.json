{
  "expo": {
    "name": "Aserpa Sa√∫de",
    "slug": "health-app",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon-aserpa.png",
    "scheme": "aserpa-saude",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/aserpa-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phillipe17macedo.healthapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-aserpa.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.phillipe17macedo.healthapp",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon-aserpa.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "onesignal-expo-plugin",
      {
        "mode": "development"
      }
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ffd01dd6-8e1a-45a7-9104-c92bcbc71b3e"
      },
      "oneSignalAppId": "1da038ce-7a5a-42f2-88c1-a119b0d94c29"
    },
    "owner": "phillipe17macedo"
  }
}
